  "email": "1217869072@qq.com",
  "build": {
    "productName": "face_data_management_tool",
    "extends": null,
    "appId": "com.alleyes.app",
    "copyright": "Copyright © alleyes",
    "extraMetadata": {
      "main": "build/electron.js",
      "asar": true
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "icons/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.png",
      "category": "public.app.face.export.tools"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/icon.png"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development",
      "maintainer": "lvpeng",
      "icon": "icons/icon.png"
    },
    "directories": {
      "buildResources": "icons",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
